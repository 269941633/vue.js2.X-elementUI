#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_mockjs@1.0.1-beta3@mockjs/bin/random" "$@"
  ret=$?
else 
  node  "$basedir/../_mockjs@1.0.1-beta3@mockjs/bin/random" "$@"
  ret=$?
fi
exit $ret
